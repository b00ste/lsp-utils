{
    "name": "@lukso/lsp6-utils",
    "version": "0.0.2",
    "sideEffects": false,
    "license": "MIT",
    "files": [
        "dist/**"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
